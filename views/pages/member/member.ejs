        $('.select-team-wrap .__select01 > .arrow1').toggleClass("arrow2");
    });
    $('.select-team .__select02').click(function () {
        $('.select-team .__list02').slideToggle("100");
        $('.select-team-wrap .__select02 > .arrow1').toggleClass("arrow2");
    });
    $('.select-team .__select03').click(function () {
        $('.select-team .__list03').slideToggle("100");
