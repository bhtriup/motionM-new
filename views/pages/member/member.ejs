    });
    $('.select-team .__select05').click(function () {
        $('.select-team .__list05').slideToggle("100");
        $('.select-team-wrap .__select05 > .arrow1').toggleClass("arrow2");
    });
    $('.select-team .__select06').click(function () {
        $('.select-team .__list06').slideToggle("100");
        $('.select-team-wrap .__select06 > .arrow1').toggleClass("arrow2");
    });
    $('.select-team .__select07').click(function () {
        $('.select-team .__list07').slideToggle("100");
        $('.select-team-wrap .__select07 > .arrow1').toggleClass("arrow2");
    });

    var div2 = document.getElementsByClassName("__select");

    function handleClick(event) {
        if (event.target.classList[1] === "active-color") {
            event.target.classList.remove("active-color");
        } else {
            for (var i = 0; i < div2.length; i++) {
                div2[i].classList.remove("active-color");
            }

            event.target.classList.add("active-color");
        }
    }
    function init() {
        for (var i = 0; i < div2.length; i++) {
            div2[i].addEventListener("click", handleClick);
        }
    }
    init();

    // 전체 체크 해제
    $("#all-mem-chk").click(function() {
        if($("#all-mem-chk").is(":checked")) $("input[name=chk]").prop("checked", true);
        else $("input[name=chk]").prop("checked", false);
    });
    $("input[name=chk]").click(function() {
        var total = $("input[name=chk]").length;
        var checked = $("input[name=chk]:checked").length;

        if(total != checked) $("#all-mem-chk").prop("checked", false);
        else $("#all-mem-chk").prop("checked", true);
    });
</script>